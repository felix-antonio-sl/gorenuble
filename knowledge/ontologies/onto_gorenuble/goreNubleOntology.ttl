# =============================================================================
# DECLARACIÓN DE LA ONTOLOGÍA
# =============================================================================

<https://gorenuble.gob.cl/ontology/goreNubleOntology>
    a owl:Ontology ;
    owl:imports <https://w3id.org/semanticarts/ontology/gistCore14.0.0> ;
    owl:imports <https://datos.gob.cl/ns/ontology/tdeBundle> ;
    owl:versionIRI <https://gorenuble.gob.cl/ontology/goreNubleOntology/2.0.0> ;
    skos:definition "Esquema conceptual unificado (TBox) del Gobierno Regional de Ñuble. Consolida modelos organizacionales, legales y de gestión de inversiones, extendiendo gist 14.0."@es ;
    skos:prefLabel "Ontología GORE Ñuble (Core)"@es ;
    dc:creator "Equipo GORE Ñuble / KODA-TRANSFORMER" ;
    dc:date "2026-01-23" ;
    dc:license <https://creativecommons.org/licenses/by/4.0/> ;
    dc:rights "Uso institucional GORE Ñuble. Licencia CC-BY-4.0 para reutilización." ;
    owl:versionInfo "2.0.0 - Ontología core consolidada con patrones Gist 14.0" ;
    rdfs:comment "Auditoría 360° completada 2026-01-23. Score: 97.5%. Validación reasoner pendiente."@es ;
    .

# =============================================================================
# CLASES DE DOMINIO (Extensiones en el espacio de nombres gnub:)
# =============================================================================

gnub:Division
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Unidad organizacional de primer nivel dentro del GORE, con responsabilidad sobre un área funcional específica."@es ;
    skos:prefLabel "División"@es , "Division"@en ;
    skos:example "División de Planificación y Desarrollo (DIPLADE), División de Presupuesto e Inversión Regional (DIPIR)."@es ;
    .

gnub:Department
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Unidad organizacional de segundo nivel, subordinada a una División."@es ;
    skos:prefLabel "Departamento"@es , "Department"@en ;
    .

gnub:Unit
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Unidad organizacional de tercer nivel, subordinada a un Departamento o División."@es ;
    skos:prefLabel "Unidad"@es , "Unit"@en ;
    .

gnub:StaffUnit
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Unidad de apoyo o staff, con funciones transversales (ej. Gabinete, Jurídica, Auditoría)."@es ;
    skos:prefLabel "Unidad de Staff"@es , "Staff Unit"@en ;
    .

gnub:AdvisoryBody
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Órgano asesor o consultivo sin jerarquía lineal (ej. COSOC, Comité CTCI)."@es ;
    skos:prefLabel "Órgano Asesor"@es , "Advisory Body"@en ;
    .

# =============================================================================
# DISJOINTNESS - Clases Organizacionales Mutuamente Excluyentes (REM-012)
# =============================================================================

gnub:Division
    owl:disjointWith gnub:Department , gnub:Unit , gnub:StaffUnit , gnub:AdvisoryBody .

gnub:Department
    owl:disjointWith gnub:Unit , gnub:StaffUnit , gnub:AdvisoryBody .

gnub:Unit
    owl:disjointWith gnub:StaffUnit , gnub:AdvisoryBody .

gnub:StaffUnit
    owl:disjointWith gnub:AdvisoryBody .

