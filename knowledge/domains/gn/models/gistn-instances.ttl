# 6. Ejemplo Completo APR Yungay
# 1. El Proyecto (IPR)
gistnr:_IPR_40056789 a gistn:Proyecto ;
    gist:name "Construcción Sistema APR Sector Los Tilos, Yungay"@es ;
    skos:prefLabel "BIP 40056789-0"@es ;
    gistn:hasBIPCode "40056789-0" ;
    gistn:hasMecanismo gistn:FRILMechanism ;
    gistn:hasTotalAmount [
        a gist:Magnitude ;
        gist:hasValue "125000"^^xsd:decimal ;
        gist:hasUnitOfMeasure gistnr:_Currency_CLP ;
    ] .

# 2. El Convenio (Agreement)
gistnr:_Convenio_Res1234 a gistn:TransferAgreement ;
    gist:name "Convenio Mandato GORE-Municipalidad Yungay"@es ;
    gistn:isBasedOnIPR gistnr:_IPR_40056789 ;
    gist:hasParty gistnr:_GoreNuble , gistnr:_Muni_Yungay .

# 3. Contrato de Obras (Resolución de Gap)
gistnr:_Contrato_Obras_001 a gistn:ConstructionContract ;
    gist:name "Contrato Obras Civiles APR Los Tilos"@es ;
    gist:hasParty gistnr:_Muni_Yungay ; # Contratante (Mandante)
    gist:isBasedOn gistnr:_Convenio_Res1234 .

# 4. Hitos y Avance
gistnr:_ActaEntregaTerreno_001 a gistn:LandDeliveryDocument ;
    gist:name "Acta Entrega Terreno APR Los Tilos"@es ;
    gist:isEvidenceFor [
        a gist:Event ;
        gist:name "Inicio de Obras APR Los Tilos"@es ;
        gist:hasActualStartDate "2024-03-15"^^xsd:date ;
    ] .

# 5. Ciclo de Vida
gistnr:_Process_APR_Yungay a gistn:IPRLifecycle ;
    gist:name "Proceso Inversión APR Los Tilos"@es ;
    gist:isCategorizedBy gistn:InProgressStatus ;
    gist:isAbout gistnr:_IPR_40056789 .

# 6. Pago y Rendición
gistnr:_Payment_APR_01 a gistn:TransferPayment ;
    gist:name "Primer Giro Transferencia - Anticipo 10%"@es ;
    gist:amount [
        a gist:Magnitude ;
        gist:hasValue "12500"^^xsd:decimal ;
        gist:hasUnitOfMeasure gistnr:_Currency_CLP ;
    ] ;
    gist:hasGiver gistnr:_GoreNuble ;
    gist:hasGetter gistnr:_Muni_Yungay ;
    gist:isBasedOn gistnr:_Convenio_Res1234 .

gistnr:_Rendition_APR_01 a gistn:Rendition ;
    gist:name "Rendición Mensual - Marzo 2024"@es ;
    gist:isCategorizedBy gistn:ApprovedStatus ;
    gistn:isRenderedBy gistnr:_Payment_APR_01 .

