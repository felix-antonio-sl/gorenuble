    skos:prefLabel "Función del GORE"@es ;
    .

gnub:PlanningInstrument
    a owl:Class ;
    rdfs:subClassOf gist:Specification ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Instrumento de planificación regional vinculante (ERD, PROT, ARI, etc.)."@es ;
    skos:prefLabel "Instrumento de Planificación"@es ;
    skos:note "Modela documentos normativos/especificaciones que guían la planificación territorial."@es ;
    .

gnub:RegionalFund
    a owl:Class ;
    rdfs:subClassOf gist:Account ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Fondo de inversión o financiamiento administrado por el GORE."@es ;
    skos:prefLabel "Fondo Regional"@es ;
    .

gnub:CompetencyType
    a owl:Class ;
    rdfs:subClassOf gist:Category ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Tipo de competencia administrativa transferible (temporal o definitiva)."@es ;
    skos:prefLabel "Tipo de Competencia"@es ;
    .

gnub:LegalMandate
    a owl:Class ;
    rdfs:subClassOf gist:Requirement ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Obligación, atribución o deber impuesto por una ley o norma jurídica a un órgano o autoridad."@es ;
    skos:prefLabel "Mandato Legal"@es ;
    skos:example "La obligación del Gobernador de someter al CORE el presupuesto regional."@es ;
    .

gnub:LegalDocument
    a owl:Class ;
    rdfs:subClassOf gist:Content ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Cuerpo normativo formal (ley, decreto, reglamento) que establece obligaciones y derechos."@es ;
    skos:prefLabel "Documento Legal"@es ;
    skos:example "DFL 1-19.175 (LOC GORE), Ley 21.033 (Creación Región de Ñuble)."@es ;
    .

gnub:LegalArticle
    a owl:Class ;
    rdfs:subClassOf gist:Content ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Artículo específico dentro de un documento legal que contiene una o más disposiciones normativas."@es ;
    skos:prefLabel "Artículo Legal"@es ;
    .

