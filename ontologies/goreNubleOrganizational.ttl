# =============================================================================
# ONTOLOGY DECLARATION
# =============================================================================

<https://gorenuble.gob.cl/ontology/goreNubleOrganizational>
    a owl:Ontology ;
    owl:imports <https://w3id.org/semanticarts/ontology/gistCore> ;
    owl:versionIRI <https://gorenuble.gob.cl/ontology/goreNubleOrganizational1.0.0> ;
    skos:definition "Extensión de Gist 14.0 para modelar la estructura organizacional del Gobierno Regional de Ñuble."@es ;
    skos:prefLabel "GORE Ñuble Organizational Ontology"@es ;
    .

# =============================================================================
# DOMAIN CLASSES (Extensions in gnub: namespace)
# =============================================================================

gnub:Division
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOrganizational> ;
    skos:definition "Unidad organizacional de primer nivel dentro del GORE, con responsabilidad sobre un área funcional específica."@es ;
    skos:prefLabel "División"@es ;
    skos:example "División de Planificación y Desarrollo (DIPLADE), División de Presupuesto e Inversión Regional (DIPIR)."@es ;
    .

gnub:Department
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOrganizational> ;
    skos:definition "Unidad organizacional de segundo nivel, subordinada a una División."@es ;
    skos:prefLabel "Departamento"@es ;
    .

gnub:Unit
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOrganizational> ;
    skos:definition "Unidad organizacional de tercer nivel, subordinada a un Departamento o División."@es ;
    skos:prefLabel "Unidad"@es ;
    .

gnub:StaffUnit
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOrganizational> ;
    skos:definition "Unidad de apoyo o staff, con funciones transversales (ej. Gabinete, Jurídica, Auditoría)."@es ;
    skos:prefLabel "Unidad de Staff"@es ;
    .

gnub:AdvisoryBody
    a owl:Class ;
    rdfs:subClassOf gist:Organization ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOrganizational> ;
    skos:definition "Órgano asesor o consultivo sin jerarquía lineal (ej. COSOC, Comité CTCI)."@es ;
    skos:prefLabel "Órgano Asesor"@es ;
    .

# =============================================================================
# CATEGORY SUBCLASSES (for flexible taxonomies)
# =============================================================================

gnub:PositionType
    a owl:Class ;
    rdfs:subClassOf gist:Category ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOrganizational> ;
    skos:definition "Tipo de cargo o posición dentro de la estructura organizacional."@es ;
    skos:prefLabel "Tipo de Cargo"@es ;
    .

gnub:OrganizationalUnitType
    a owl:Class ;
    rdfs:subClassOf gist:Category ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOrganizational> ;
    skos:definition "Tipo de unidad organizacional (ej. División, Departamento, Unidad)."@es ;
    skos:prefLabel "Tipo de Unidad Organizacional"@es ;
    .

