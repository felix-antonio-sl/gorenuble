    skos:prefLabel "Subtítulo Presupuestario"@es ;
    .

gnub:BudgetPartida
    a owl:Class ;
    rdfs:subClassOf gist:Category ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Nivel superior de la clasificación institucional del presupuesto (ej. Partida 31)."@es ;
    skos:prefLabel "Partida Presupuestaria"@es ;
    .

gnub:BudgetChapter
    a owl:Class ;
    rdfs:subClassOf gist:Category ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Desagregación institucional de una Partida (ej. Capítulo 01)."@es ;
    skos:prefLabel "Capítulo Presupuestario"@es ;
    .

gnub:BudgetProgram
    a owl:Class ;
    rdfs:subClassOf gist:Category ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "División presupuestaria base asignada a un servicio o propósito específico (ej. Programa 19: GORE Ñuble)."@es ;
    skos:prefLabel "Programa Presupuestario"@es ;
    .

gnub:GlossType
    a owl:Class ;
    rdfs:subClassOf gist:Category ;
    rdfs:isDefinedBy <https://gorenuble.gob.cl/ontology/goreNubleOntology> ;
    skos:definition "Tipo de glosa presupuestaria según su naturaleza y estructura."@es ;
    skos:prefLabel "Tipo de Glosa"@es ;
