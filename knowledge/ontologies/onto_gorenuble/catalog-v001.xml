<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--
  NOTA DE PORTABILIDAD (REM-018)
  ==============================
  Este catálogo usa rutas relativas que asumen la siguiente estructura:
  
  Developer/
  ├── gorenuble/knowledge/ontologies/onto_gorenuble/  (este directorio)
  ├── tde/knowledge/ontologies/onto_tde/
  └── fxsl/knowledge/domains/gist/ontologies/
  
  Si los repositorios se reorganizan, actualizar las rutas relativas.
  Para usar con Protégé, abrir goreNubleBundle.ttl desde este directorio.
-->
<catalog prefer="public" xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">
    <!-- =========================================================== -->
    <!-- Gist Core: Upper Ontology -->
    <!-- =========================================================== -->
    <uri name="https://w3id.org/semanticarts/ontology/gistCore14.0.0"
        uri="../../../../fxsl/knowledge/domains/gist/ontologies/gistCore14.0.0.ttl" />

    <!-- =========================================================== -->
    <!-- TDE: Mapear IRI principal al BUNDLE para carga completa -->
    <!-- =========================================================== -->
    <uri name="https://datos.gob.cl/ns/ontology/tde"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdeBundle.ttl" />

    <!-- TDE Bundle (explícito) -->
    <uri name="https://datos.gob.cl/ns/ontology/tdeBundle"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdeBundle.ttl" />

    <!-- TDE Módulos individuales (el bundle los importa) -->
    <uri name="https://datos.gob.cl/ns/ontology/tdeCore"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdeCore.ttl" />
    <uri name="https://datos.gob.cl/ns/ontology/tdeProcesos"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdeProcesos.ttl" />
    <uri name="https://datos.gob.cl/ns/ontology/tdeDatos"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdeDatos.ttl" />
    <uri name="https://datos.gob.cl/ns/ontology/tdePrincipios"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdePrincipios.ttl" />
    <uri name="https://datos.gob.cl/ns/data/tdeReferenceData"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdeReferenceData.ttl" />
    <uri name="https://datos.gob.cl/ns/instance/tdeInstances"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdeInstances.ttl" />
    <uri name="https://datos.gob.cl/ns/lexicon/tdeLexicon"
        uri="../../../../tde/knowledge/ontologies/onto_tde/tdeLexicon.ttl" />

    <!-- =========================================================== -->
    <!-- GNUB: Ontología y Datos del GORE Ñuble -->
    <!-- =========================================================== -->
    <uri name="https://gorenuble.gob.cl/ontology/goreNubleOntology"
        uri="goreNubleOntology.ttl" />
    <uri name="https://gorenuble.gob.cl/ontology/alignmentGnubTde"
        uri="alignmentGnubTde.ttl" />
    <uri name="https://gorenuble.gob.cl/ontology/goreNubleBundle"
        uri="goreNubleBundle.ttl" />
    <uri name="https://gorenuble.gob.cl/data/goreNubleOrgData"
        uri="goreNubleOrgData.ttl" />
    <uri name="https://gorenuble.gob.cl/data/goreNubleIPRData"
        uri="goreNubleIPRData.ttl" />
    <uri name="https://gorenuble.gob.cl/data/goreNubleLegalData"
        uri="goreNubleLegalData.ttl" />
    <uri name="https://gorenuble.gob.cl/data/goreNubleRenditionData"
        uri="goreNubleRenditionData.ttl" />
    <uri name="https://gorenuble.gob.cl/data/goreNubleApprovalData"
        uri="goreNubleApprovalData.ttl" />
    <uri name="https://gorenuble.gob.cl/data/goreNubleReferenceData"
